<h3> Student Form </h3>

<%= form_tag students_path do %>
  <label>Student First Name:</label><br>
  <%= text_field_tag :'student[title]' %><br>

  <label>School Class Room Number:</label><br>
  <%= text_field_tag :'school_class[room_number]' %><br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <input type="submit" value="Create School Class">
<%end%>
